<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="395.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="app.HomeController">

   <children>
      <ButtonBar id="search" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Weather Location Searcher:" />
            <ChoiceBox fx:id="locationTypeChoiceBox" prefWidth="150.0" />
            <TextField nodeOrientation="LEFT_TO_RIGHT" text="City: Ex. San Jose" />
          <Button mnemonicParsing="false" text="Search" />
        </buttons>
      </ButtonBar>
   </children>

   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
